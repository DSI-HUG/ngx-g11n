{
    "name": "@hug/ngx-g11n",
    "version": "1.1.1",
    "description": "Angular helpers for internationalizing and localizing your application",
    "homepage": "https://github.com/dsi-hug/ngx-g11n",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-hug/ngx-g11n.git"
    },
    "keywords": [
        "angular",
        "localization",
        "internationalization",
        "i18n",
        "l10n",
        "g11n",
        "translate",
        "translation",
        "multilingual",
        "language",
        "locale",
        "ngx-g11n",
        "angular-localization",
        "angular-tools",
        "angular-helpers"
    ],
    "sideEffects": false,
    "scripts": {
        "start": "node ./make.mjs watch",
        "lint": "node ./make.mjs lint",
        "test:lib": "node ./make.mjs test-lib",
        "test:schematics": "node ./make.mjs test-schematics",
        "test:ci": "node ./make.mjs test-ci",
        "build": "node ./make.mjs build",
        "build:global": "node ./make.mjs build-global",
        "release": "npm publish ../../dist/ngx-g11n --access=public --workspaces=false"
    },
    "peerDependencies": {
        "@angular/common": ">= 14",
        "@angular/core": ">= 14",
        "@angular/localize": ">= 14",
        "rxjs": ">= 7",
        "@angular/material": ">= 14",
        "@angular/material-date-fns-adapter": ">= 14",
        "date-fns": ">= 2.3"
    },
    "peerDependenciesMeta": {
        "@angular/localize": {
            "optional": true
        },
        "@angular/material": {
            "optional": true
        },
        "@angular/material-date-fns-adapter": {
            "optional": true
        },
        "date-fns": {
            "optional": true
        }
    },
    "dependencies": {
        "@hug/ngx-schematics-utilities": "^10.2.2",
        "tslib": "^2.8.1"
    },
    "schematics": "./schematics/collection.json",
    "ng-add": {
        "save": "dependencies"
    }
}
